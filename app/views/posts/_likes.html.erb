<% if logged_in? && current_user.likes?(post) %>
<%= link_to "Unlike", post_like_path(post), class: "btn btn-info btn-sm buticons glyphicon glyphicon-fire", method: :delete, remote: true %>
<% else %>
<%= link_to "Like", post_like_path(post), class: "btn btn-info btn-sm buticons glyphicon glyphicon-fire",method: :post, remote: true %>
<% end %>
<%= post.likes.count %>
